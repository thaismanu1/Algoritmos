#include <iostream>
#include <iomanip>

using namespace std;

int main()
{
    int n; // tamanho do vetor
    int *p; // ponteiro para percorrer o vetor
    int *v = NULL; // vetor

    cin >> n; // entrando com o tamanho desejado para o vetor

    v = new int[n]; // alocando memoria

    p = v;

    // inserindo elementos no vetor
    for (int i = 0; i < n; i++) {
        cin >> *p;
        p++;
    }

    p = v; // reiniciando o ponteiro para percorrer o vetor

    // mostrando elementos do vetor
    for (int i = 0; i < n; i++) {
        cout << *p << " ";
        p++;
    }

    delete[] v;

    return 0;
}
