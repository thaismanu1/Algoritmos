#include <iostream>

using namespace std;

int main()
{
	int vet[30]; //elementos do vetor
	int n; //tamanho do vetor
	int maior = 0;
	int menor = 1000;
	
	cin >> n;
	
	for(int i=0; i<n; i++)
	{
		cin >> vet[i];
		if (vet[i] > maior)
		{
			maior = vet[i];
		}
		if (vet[i] < menor)
		{
			menor = vet[i];
		}
	}
	
	cout << "Menor numero de contatos: " << menor << endl;
	cout << "Maior numero de contatos: " << maior << endl;
	
	return 0;
}
